# Custom matplotlibrc configuration file.  Generic version is here:
# site-packages/matplotlib/mpl-data/matplotlibrc.


#### CONFIGURATION BEGINS HERE

backend      : Qt5Agg
#interactive  : False


### LINES
lines.linestyle : -
lines.linewidth : 2.0
lines.marker : o
lines.markeredgewidth : 1.0
lines.markersize : 4.0
lines.antialiased : False


### PATCHES
patch.edgecolor : eeeeee
patch.facecolor : blue


### AXES
axes.edgecolor : bcbcbc
axes.facecolor : eeeeee
axes.grid : True
axes.labelsize : large
axes.prop_cycle : cycler('color', ['xkcd:' + color for color in ['cadet blue', 'scarlet', 'bluegreen', 'military green', 'british racing green', 'midnight', 'rose red', 'cyan', 'gold', 'bluish purple']]) + cycler('linestyle', ['-', '--', '-.', ':', '-', '--', '-.', ':', '-', '--'])
axes.titlepad : 10.0
axes.titlesize : x-large


### GRIDS
grid.alpha : 0.5
grid.linestyle : dashed
grid.linewidth : 1.0


### FIGURE
figure.figsize : 8.0, 5.0
figure.max_open_warning : 10
